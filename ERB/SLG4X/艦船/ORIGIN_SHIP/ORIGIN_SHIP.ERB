FOR SLOT_ID, 0, ORIGIN_SHIP_MAIN_WEAPON_SLOT:ID
	TRYCALLFORM GET_MAIN_WEAPON_NAME_{SHIP_EQUIP:主砲:SLOT_ID:SHIP_ID}
