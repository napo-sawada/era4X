			D = Z:2
		SIF E:COUNT == E:1
			G = Z:2
		SIF E:COUNT == E:2
			H = Z:2
		SIF E:COUNT == E:3
			I = Z:2
	ENDIF
	IF M:3 == E:COUNT
		Z:3 = C
		TIMES Z:3 , 0.10
		SIF E:COUNT == E:0
			D = Z:3
		SIF E:COUNT == E:1
