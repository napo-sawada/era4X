	Y = COUNT+1
	IF M:COUNT < M:Y
		X = M:COUNT
		M:COUNT = M:Y
		M:Y = X
	ENDIF
REND
REPEAT 2
	Y = COUNT+1
	IF M:COUNT < M:Y
		X = M:COUNT
		M:COUNT = M:Y
		M:Y = X
	ENDIF
REND

IF M:0 < M:1
	X = M:0
	M:0 = M:1
	M:1 = X
ENDIF

