SIF BASE:A:0 > 800
	PRINTL [2]   - 黄昏の森
SIF BASE:A:0 > 1000
	PRINTL [3]   - ネザー
SIF BASE:A:0 > 1200
	PRINTL [4]   - 舞倉市郊外
PRINTL [100] - やめる
DRAWLINE
$INPUT_LOOP_2
INPUT
IF RESULT == 0
	B = 1
ELSEIF RESULT == 1 && BASE:A:0 > 500
	B = 2
ELSEIF RESULT == 2 && BASE:A:0 > 800
	B = 3
ELSEIF RESULT == 3 && BASE:A:0 > 1000
	B = 4
ELSEIF RESULT == 4 && BASE:A:0 > 1200
	B = 5
ELSEIF RESULT == 100
	RETURN 0
ELSE
	GOTO INPUT_LOOP_2
ENDIF

